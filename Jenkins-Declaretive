pipeline{
    agent any
    
    stages{
        stage('checkout'){
            steps {
                git branch: 'main', url: 'https://github.com/VenkySeetha/Venky.git'
            }
        }
        stage('build'){
            steps{
                echo 'Welcome To Maven'
            }
        }
        stage('deploy'){
            steps{
                echo 'Deploy'
            }
        }
    }
}

