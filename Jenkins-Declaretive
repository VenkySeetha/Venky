pipeline{
    agent any
    
    stages{
        stage('checkout'){
            steps {
                git branch: 'main', url: 'https://github.com/VenkySeetha/Venky.git'
            }
        }
        stage('Maven Clean Build'){
            steps{
                def mavenHome = tool name: "Maven",type: "maven"
                def mavenCMD = "${mavenHome}/bin/mvn"
                sh "${mavenCMD} clean package"
            }
        }
        stage('deploy'){
            steps{
                echo 'Deploy'
            }
        }
    }
}

