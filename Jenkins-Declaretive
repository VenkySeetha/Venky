pipeline {
    agent any
    options {
        // Timeout counter starts AFTER agent is allocated
        timeout(time: 1, unit: 'SECONDS')
    }
    stages {
        stage('Example') {
            steps {
                echo 'Hello world'
            }
        }
    stage('Example Build') {
            steps {
                sh 'mvn -B clean verify'
            }
        }
    }
}
